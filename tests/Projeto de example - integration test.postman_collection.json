	"event": [
		{
			"listen": "prerequest",
			"script": {
				"type": "text/javascript",
				"exec": [
					"postman.clearEnvironmentVariable(\"base_url_api\");",
					"postman.clearEnvironmentVariable(\"base_url_token\");",
					"",
					"console.log(\"Definindo URL base do projeto\");",
					"var urlProject = pm.collectionVariables.get(\"base_url_api_local\"); /*Valor do ambiente testado*/",
					"",
					"if(urlProject == '' || urlProject == null)",
					"{",
					"    throw new Error('Url base do projeto esta nulla !!');",
					"}",
					"",
					"console.log(\"Url base do ambinete de teste\", urlProject);",
					"postman.setEnvironmentVariable(\"base_url_api\", urlProject);",
					"",
					"console.log(\"Definindo URL base de autenticação\");",
					"var urlLogin = pm.collectionVariables.get(\"base_url_login_local\"); /*Valor do ambiente testado*/",
					"",
					"if(urlLogin == '' || urlLogin == null)",
					"{",
					"    throw new Error('Url base do token esta nulla !!');",
					"}",
					"",
					"console.log(\"Url base do ambinete de teste\", urlLogin);",
					"postman.setEnvironmentVariable(\"base_url_token\", urlLogin);"
				]
			}
